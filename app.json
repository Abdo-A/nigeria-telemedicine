{
  "expo": {
    "name": "Fortitudo Telemedicine",
    "slug": "fortitudo-telemedicine",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon192x192.png",
